{
  "version": 2,
  "builds": [
    {
      "src": "bookverse/api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/generate_todo.js",
      "use": "@vercel/node"
    },
    {
      "src": "BookVerse.html",
      "use": "@vercel/static"
    },
    {
      "src": "partial-2.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/bookverse/api/chat",
      "dest": "/bookverse/api/chat.js"
    },
    {
      "src": "/api/generate_todo",
      "dest": "/api/generate_todo.js"
    },
    {
      "src": "/bookverse/(.*)",
      "dest": "/bookverse/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/partial-2.html"
    }
  ]
}
