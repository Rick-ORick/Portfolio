{
  "version": 2,
  "builds": [
    {
      "src": "bookverse/api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "BookVerse.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/bookverse/api/chat",
      "dest": "/bookverse/api/chat.js"
    },
    {
      "src": "/bookverse/(.*)",
      "dest": "/bookverse/$1"
    }
  ]
}
