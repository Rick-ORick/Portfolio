{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/generate_todo.js",
      "use": "@vercel/node"
    },
    {
      "src": "bookverse/BookVerse.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "api/generate.js"
    },
    {
      "src": "/api/generate_todo",
      "dest": "api/generate_todo.js"
    },
    {
      "src": "/(.*)",
      "dest": "bookverse/$1"
    }
  ]
}

